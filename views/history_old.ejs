<% include ../views/layouts/header %>
<% include ../views/layouts/menu %>
                    </div>
                    <!-- /#left -->
                <div id="content">
                    <div class="outer">
                        <div class="inner bg-light lter">

<div class="row">
        <div class="col-lg-12">
            <table id="dataTable" class="table table-bordered table-condensed table-hover table-striped">
                <tr>
                    <th>#history</th>
                    <th>Project Name</th>
                    <th>#project</th>
                    <th>User</th>
                    <th>Type</th>
                    <th>Method</th>
                    <th>Result</th>
                    <th>Date</th>
                    <th>Time</th>
                    <th>Description</th>
                </tr>
                <% var count=0; %>
            <% for (var i=0; i < projects.length; i++) { %>
                <% for (var j=0; j < projects[i].log.length; j++) { %>
                    <tr>
                        <td><%= ++count %></td>
                        <td><%= projects[i].name %></td>
                        <td><%= j+1 %></td>
                        <td><%= projects[i].log[j].user.username %></td>
                        <td><%= projects[i].log[j].action.type %></td>
                        <td><%= projects[i].log[j].action.method %></td>
                        <td><%= projects[i].log[j].action.result %></td>
                        <td><%= projects[i].log[j].created.date %></td>
                        <td><%= projects[i].log[j].created.time %></td>
                        <td><%= projects[i].log[j].description %></td>
                    </tr>
                <% } %>
                <% for (var j=0; j < projects[i].tasks[j].log.length; j++) { %>
                    <tr>
                        <td><%= ++count %></td>
                        <td><%= projects[i].tasks[j].name %></td>
                        <td><%= j+1 %></td>
                        <td><%= projects[i].tasks[j].log.user %></td>
                        <td><%= projects[i].tasks[j].log.action %></td>
                        <td><%= projects[i].tasks[j].log.action %></td>
                        <td><%= projects[i].tasks[j].log.action %></td>
                        <td><%= projects[i].tasks[j].log.created %></td>
                        <td><%= projects[i].tasks[j].log.created %></td>
                        <td><%= projects[i].tasks[j].log.description %></td>
                    </tr>
                <% } %>
            <% } %>
            </table>
        </div>
</div>
<hr>

                        </div>
                        <!-- /.inner -->
                    </div>
                    <!-- /.outer -->
                </div>
                <!-- /#content -->

            </div>
            <!-- /#wrap -->
